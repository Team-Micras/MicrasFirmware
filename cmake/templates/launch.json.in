{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "openocd",
            "openOCDLaunchCommands": ["gdb port 3333", "tcl port 6666", "telnet port 4444"],
            "overrideAttachCommands": ["target-select extended-remote localhost:3333"],
            "cwd": "${CMAKE_CURRENT_SOURCE_DIR}",
            "gdbPath": "gdb-multiarch",
            "executable": "build/${DEBUG_FILE_NAME}.elf",
            "name": "Cortex Debug (OpenOCD)",
            "runToEntryPoint": "main",
            "configFiles":
            [
                "${OPENOCD_SCRIPTS_PATH}/interface/stlink.cfg",
                "${OPENOCD_SCRIPTS_PATH}/target/${TARGET_CFG}x.cfg"
            ]
        },
        {
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "jlink",
            "cwd": "${CMAKE_CURRENT_SOURCE_DIR}",
            "gdbPath": "gdb-multiarch",
            "executable": "build/${DEBUG_FILE_NAME}.elf",
            "name": "Cortex Debug (J-Link)",
            "device": "${DEVICE}",
            "interface": "swd",
        },
        {
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "stutil",
            "cwd": "${CMAKE_CURRENT_SOURCE_DIR}",
            "gdbPath": "gdb-multiarch",
            "executable": "build/${DEBUG_FILE_NAME}.elf",
            "name": "Cortex Debug (ST-Util)",
            "device": "${DEVICE}",
            "v1": false
        },
    ]
}
