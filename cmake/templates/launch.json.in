{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "external",
            "cwd": "${CMAKE_CURRENT_SOURCE_DIR}",
            "preLaunchTask": "WSL launch openocd",
            "gdbTarget": "localhost:3333",
            "gdbPath": "gdb-multiarch",
            "executable": "build/${DEBUG_FILE_NAME}.elf",
            "name": "Cortex Debug (OpenOCD)",
            "runToEntryPoint": "main",
        },
        {
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "jlink",
            "cwd": "${CMAKE_CURRENT_SOURCE_DIR}",
            "gdbPath": "gdb-multiarch",
            "executable": "build/${DEBUG_FILE_NAME}.elf",
            "name": "Cortex Debug (J-Link)",
            "device": "${DEVICE}",
            "interface": "swd",
        },
        {
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "stutil",
            "cwd": "${CMAKE_CURRENT_SOURCE_DIR}",
            "gdbPath": "gdb-multiarch",
            "executable": "build/${DEBUG_FILE_NAME}.elf",
            "name": "Cortex Debug (ST-Util)",
            "device": "${DEVICE}",
            "v1": false
        },
    ]
}
